# This file is overwritten upon Agent upgrade.
# To make modifications to the check configuration, please copy this file
# to `agent_metrics.yaml` and make your changes on that file.

init_config:
    process_metrics:
        - name: memory_info
          type: gauge
          active: yes
        - name: io_counters
          type: rate
          active: yes
        - name: num_threads
          type: gauge
          active: yes
        - name: connections
          type: gauge
          active: no

instances:
    [{}]
